<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:mk.icelabs.gwt.polymer.client.ui.polymer.core"
	xmlns:p="urn:import:mk.icelabs.gwt.polymer.client.ui.polymer.paper">
	<ui:style>
	.important {
		font-weight: bold;
	}
	.center {
	
		width: 100%;
		height: 100%;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.center}">
		<c:CoreToolbar  ui:field="navigation" ></c:CoreToolbar>
		<c:CoreAnimatedPages ui:field="toolbars" styleName="{style.center}"></c:CoreAnimatedPages>
	</g:HTMLPanel>
</ui:UiBinder> 